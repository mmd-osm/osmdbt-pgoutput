
#include <osmium/version.hpp>

#include <string>

std::string get_osmdbt_version() { return "@PROJECT_VERSION@"; }

char const *get_osmdbt_long_version() noexcept
{
    return "osmdbt-pgoutput version @PROJECT_VERSION@@VERSION_FROM_GIT@";
}

char const *get_libosmium_version() noexcept
{
    return "libosmium version " LIBOSMIUM_VERSION_STRING;
}
